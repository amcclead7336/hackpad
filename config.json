		{
		"title":"Scan/Enum",
		"keys":[
			{
                "lname": "Baseline nmap scan",
				"qname": "NMAP1",
				"ctext": "nmap -p- -sT -sC -sV -O -T4 {RHOST} -oA {File}",
				"parms": ["RHOST", "File"],
                "des": "Scans all ports with TCP Scan, Script scan, Version scan, and OS scan. It outputs data to File"
            },
			{
                "lname": "SMB Enum nmap scan",
				"qname": "SMBenum",
				"ctext": "nmap -p 445 --script=smb-enum-shares.nse,smb-enum-users.nse {RHOST} -oA {File}",
				"parms": ["RHOST", "File"],
                "des": "Nmap scan that uses common smb enum scripts"
            },
			{
                "lname": "SMB Enum List Shares",
				"qname": "SMBls",
				"ctext": "smbclient -L \\\\\\\\{RHOST}",
				"parms": ["RHOST"],
                "des": "List Available Shares"
            },
			{
                "lname": "SNMP Enum",
				"qname": "SNMPenum",
				"ctext": "onesixtyone -c {CommunityStr} {RHOST}",
				"parms": ["CommunityStr", "RHOST"],
                "des": "Can Enumerate SNMP by brute forcing community strings. Can be downloaded from here https://github.com/trailofbits/onesixtyone/blob/master/dict.txt"
            }
		]
